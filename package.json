{
  "name": "cljs-dev-template",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "imports": {
    "#cljs/*": "./cljs-dist/*"
  },
  "scripts": {
    "app-dev": "npm-run-all -p app-cljs-dev app-js-dev-wait",
    "app-cljs-dev": "shadow-cljs watch :app",
    "app-cljs-prod": "shadow-cljs release :app --pseudo-names",
    "app-js-dev-wait": "wait-on ./cljs-dist/app/app.main.js && npm run app-js-dev",
    "app-js-dev": "vite --config vite.config.js --port 4000 src/app/",
    "app-js-prod": "vite build --emptyOutDir --outDir ../dist/app src/app/",
    "clean": "rm -r cljs-dist/* dist/*",
    "clear-cache": "rm -r .shadow-cljs/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "clj-kondo": "^2023.5.26",
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "^2.25.3",
    "typescript": "^5.3.3",
    "vite": "^4.2.1",
    "wait-on": "^7.0.1"
  }
}
